platform :ios, '9.0'

def shared_pods
  # Pods for AppCenter
  pod 'AppCenter/Crashes', '~> 1.11.0'
  pod 'AppCenter/Analytics', '~> 1.11.0'
  pod 'AppCenterReactNativeShared', '~> 1.10.0'
  platform :ios, '9.0'
end

target 'ReactNativeSeed' do
  shared_pods
end

target 'ReactNativeSeed Debug' do
  shared_pods
end

target 'ReactNativeSeed Staging' do
  shared_pods
end
