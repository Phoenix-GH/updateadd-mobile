{
  "name": "ReactNativeSeed",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "clean-android": "cd android; ./gradlew clean",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && yarn cache clean",
    "test": "jest",
    "lint": "./node_modules/.bin/eslint .",
    "lint:fix": "./node_modules/.bin/eslint . --fix",
    "flow": "./node_modules/.bin/flow",
    "coverage": "NODE_ENV=development node_modules/.bin/nyc --report-dir coverage --reporter=lcov jest",
    "test:watch": "NODE_ENV=development yarn test --watch",
    "build": "flow-typed install",
    "prepublish": "yarn run build"
  },
  "dependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-syntax-optional-chaining": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@storybook/addon-a11y": "^4.0.2",
    "@storybook/addon-actions": "^4.0.2",
    "@storybook/addon-info": "^4.0.2",
    "@storybook/addon-knobs": "^4.0.2",
    "@storybook/addon-links": "^4.0.2",
    "@storybook/addon-viewport": "^4.0.2",
    "@storybook/react": "^4.0.2",
    "appcenter": "^1.3.0",
    "appcenter-analytics": "^1.3.0",
    "appcenter-crashes": "^1.3.0",
    "axios": "^0.18.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-flow-react-proptypes": "^24.1.2",
    "babel-plugin-react-css-modules": "^4.0.0",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "eslint-config-airbnb": "^17.1.0",
    "immutability-helper": "^2.6.5",
    "lodash": "^4.17.11",
    "moment": "2.22.2",
    "react": "^16.6.1",
    "react-devtools": "^3.1.0",
    "react-native": "0.57.7",
    "react-native-code-push": "^5.4.2",
    "react-native-config": "^0.11.7",
    "react-native-gesture-handler": "^1.0.10",
    "react-native-sentry": "^0.39.1",
    "react-native-vector-icons": "^6.1.0",
    "react-navigation": "^3.0.5",
    "react-navigation-redux-helpers": "^2.0.8",
    "react-redux": "^5.1.1",
    "redux": "^4.0.1",
    "redux-saga": "^0.16.2",
    "remote-redux-devtools": "^0.5.13",
    "remote-redux-devtools-on-debugger": "^0.8.3"
  },
  "devDependencies": {
    "@babel/plugin-transform-flow-strip-types": "^7.2.0",
    "@babel/preset-flow": "^7.0.0",
    "babel-cli": "^6.26.0",
    "babel-preset-react-native": "5.0.2",
    "eslint": "^5.9.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-flowtype": "3.2.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jest": "^22.1.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-react-native": "3.5.0",
    "flow-bin": "^0.87.0",
    "flow-typed": "^2.5.1",
    "husky": "^1.2.0",
    "jest": "^23.6.0",
    "react-test-renderer": "^16.6.3"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "<rootDir>/__mocks__",
      "<rootDir>/node_modules/appcenter/test/AppCenterMock.js",
      "<rootDir>/node_modules/appcenter-analytics/test/AppCenterAnalyticsMock.js",
      "<rootDir>/node_modules/appcenter-crashes/test/AppCenterCrashesMock.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-navigation|react-navigation-redux-helpers)"
    ],
    "collectCoverage": true
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint && yarn flow && yarn test -u"
    }
  }
}
